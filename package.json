{
  "name": "order-management-service",
  "version": "1.0.0",
  "description": "A boiler plate project for NodeJs",
  "main": "server.js",
  "author": "Cimpress",
  "scripts": {
    "watch": "watchify -d public/scripts/app.js -o public/scripts/bundle.js -v",
    "build": "browserify public/scripts/app.js -o public/scripts/bundle.js",
    "test": "mocha --reporter list app/tests/*.js",
    "start": "npm run watch & nodemon server.js"
  },
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "babel-core": "~6.3.26",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "~6.3.13",
    "babel-preset-react": "~6.3.13",
    "babelify": "~7.2.0",
    "body-parser": "~1.4.2",
    "bower": "~1.3.12",
    "browserify": "~6.0.3",
    "config": "^1.21.0",
    "express": "~4.11.1",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "method-override": "~2.0.2",
    "minimist": "*",
    "mongoose": "~3.8.0",
    "nodemon": "~1.8.1",
    "react": "~0.14.4",
    "react-dom": "~0.14.4",
    "restler": "^3.4.0",
    "swagger-node-express": "~2.0",
    "uglify-js": "~2.4.15",
    "underscore": "^1.8.3",
    "watchify": "~3.6.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-config-google": "^0.5.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0"
  }
}
